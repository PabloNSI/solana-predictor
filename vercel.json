{
  "functions": {
    "api/predict.py": {
      "maxDuration": 10,
      "memory": 1024
    },
    "api/health.py": {
      "maxDuration": 3,
      "memory": 256
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "buildCommand": "pip install -r requirements.txt",
  "outputDirectory": ".",
  "installCommand": "pip install -r requirements.txt"
}